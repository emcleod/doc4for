{
    "output_dir": "docs",
    "extensions": ["f90", "f95", "f03", "f08", "f77"],
    "output_formats": {
      "html": {
        "enabled": true,
        "templates": {
          "root_dir": "templates",
          "core": {
            "dir": "html",
            "inheritance_tree": "inheritance_tree_template.html",
            "file": "file_template.html",
            "module": "module_template.html"
          },
          "static": {
            "css": "static/css",
            "images": "static/images"
          }
        },
        "syntax_highlighting": true,
        "single_page": false
      },
      "pdf": {
        "enabled": false
      },
      "markdown": {
        "enabled": false
      }
    },
    "exclude_dirs": ["docs", ".git", "__pycache__", "build", "dist", "myenv"],
    "author": null,
    "title": null,
    "version": null,
    "include_private": false
  }